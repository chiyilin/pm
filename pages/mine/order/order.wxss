.page-todos {
  display: flex;
  flex-direction: column;
  width: 100%;
  min-height: 100vh;
  background-color: #f0eff4;
  font-size: 28rpx;
  color: #4f4f4f;
}

.bg {
  height: 120rpx;
  line-height: 120rpx;
  font-size: 34rpx;
  color: #fff;
  background: url(data:image/jpeg;base64,);
}

.bg .xq {
  margin-left: 88rpx;
}

.first {
  background-color: #fff;
  height: 140rpx;
  padding-left: 25rpx;
  position: relative;
}

.addre {
  display: block;
  margin-top: 15rpx;
}

.tel {
  position: absolute;
  right: 25rpx;
  top: 34rpx;
}

.name {
  margin-top: 35rpx;
  display: inline-block;
}

.second {
  background-color: #fff;
  position: relative;
}

.second text {
  display: block;
  margin: 25rpx;
}

.copy1 {
  border: 1px solid #7b7b7b;
  padding: 7rpx 13rpx;
  border-radius: 3px;
  position: absolute;
  top: -6rpx;
  right: 0rpx;
  font-size: 22rpx;
}

.third {
  background-color: #fff;
  margin-top: 20rpx;
}

.info-box {
  display: block;
  padding: 20rpx;
}

.yf {
  position: absolute;
  left: 25rpx;
  top: 123rpx;
}

.zonge {
  position: absolute;
  top: 36rpx;
  right: 25rpx;
}

.shuliang {
  position: absolute;
  top: 93rpx;
  right: 25rpx;
}

.money {
  position: absolute;
  right: 25rpx;
  top: 130rpx;
}

.box {
  margin-top: 25rpx;
  border-top: 1px solid #dedede;
  height: 60rpx;
  line-height: 60rpx;
  text-align: right;
}

.box text {
  margin-right: 25rpx;
}

.qian {
  color: red;
}

.fore {
  background-color: #fff;
  margin-top: 20rpx;
  height: 240rpx;
  position: relative;
}

.fore image {
  width: 154rpx;
  height: 154rpx;
  position: absolute;
  left: 25rpx;
  top: 34rpx;
}

.title {
  width: 520rpx;
  display: block;
  position: absolute;
  top: 34rpx;
  right: 25rpx;
  height: 100rpx;
  overflow: hidden;
  line-height: 34rpx;
}

.guige {
  position: absolute;
  top: 147rpx;
  left: 196rpx;
}

.shul {
  position: absolute;
  right: 25rpx;
  top: 147rpx;
}

.fore .qian {
  position: absolute;
  right: 25rpx;
  bottom: 25rpx;
}

.btn {
  margin-top: 20rpx;
  padding: 10rpx;
  vertical-align: middle;
  background-color: #fff;
  border-top: 1px solid #e4e4e6;
}

.btn button {
  float: right;
  width: 164rpx;
  height: 63rpx;
  margin-right: 20rpx;
  line-height: 63rpx;
  font-size: 28rpx;
  text-align: center;
}

.btn1 {
  color: red;
  border: 1px solid red;
  background-color: #fff;
  border-radius: 3rpx;
}

.btn2 {
  border: 1px solid red;
  border-radius: 3px;
  background-color: red;
  color: #fff;
}

.btn3 {
  color: #666;
  border: 1px solid #666;
  background-color: #fff;
  border-radius: 3rpx;
}

.radius {
  border-radius: 100rpx;
}
